{
  "name": "@stackstorm/st2flow-header",
  "version": "1.0.0-pre.5",
  "description": "",
  "private": true,
  "main": "header.component.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackstorm/st2flow.git"
  },
  "bugs": {
    "url": "https://github.com/stackstorm/st2flow/issues"
  },
  "homepage": "https://github.com/stackstorm/st2flow#readme",
  "browserify": {
    "transform": [
      "babelify",
      [
        "@stackstorm/browserify-postcss",
        {
          "extensions": [
            ".css"
          ],
          "inject": "insert-css",
          "modularize": true,
          "plugin": [
            "postcss-import",
            "postcss-nested",
            [
              "postcss-preset-env",
              {
                "features": {
                  "custom-properties": {
                    "preserve": false
                  }
                }
              }
            ]
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@stackstorm/st2-style": "^2.0.0",
    "classnames": "2.2.6",
    "insert-css": "2.0.0",
    "prop-types": "15.6.2"
  },
  "devDependencies": {
    "@stackstorm/browserify-postcss": "0.3.4-patch.5",
    "babelify": "8.0.0",
    "postcss": "6.0.22",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "postcss-preset-env": "5.1.0"
  },
  "peerDependencies": {
    "react": "16.4.1"
  }
}
